<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gx.dao.DetailMapper">
	<resultMap id="BaseResultMap" type="com.gx.po.Detail">

		<constructor>
			<arg column="id" javaType="java.lang.Integer" jdbcType="INTEGER" />
			<arg column="campaign_id" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="list_id" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="lead_id" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="start_date" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="end_date" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="phone_number" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="uniqueid" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="status" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="achieve_date" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="call_time" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="ring_time" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="talk_time" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="customer_type" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="download" javaType="java.lang.String"
				jdbcType="VARCHAR" />
			<arg column="callTimes" javaType="java.lang.String"
				jdbcType="VARCHAR" />


		</constructor>
	</resultMap>



	<sql id="Base_Column_List">

		id,
		campaign_id,
		list_id,
		lead_id,
		start_date,
		end_date,
		phone_number,
		uniqueid,
		status,
		achieve_date,
		call_time,
		ring_time,
		talk_time,
		customer_type,
		download,
		callTimes


	</sql>


	<select id="selectByPrimaryKeyforReportDetail"
		parameterType="java.lang.String" resultMap="BaseResultMap">

		select
		<include refid="Base_Column_List" />
		from ai_report_detail
		where id = #{id,jdbcType=INTEGER}
	</select>


	<insert id="insert" parameterType="com.gx.po.Detail">

		insert into ai_report_detail (

		campaign_id,
		list_id,
		lead_id,
		start_date,
		end_date,
		phone_number,
		uniqueid,
		status,
		achieve_date,
		call_time,
		ring_time,
		talk_time,
		customer_type,
		download,
		callTimes


		) values (
		
		#{campaign_id,jdbcType=VARCHAR},
		#{list_id,jdbcType=VARCHAR},
		#{lead_id,jdbcType=VARCHAR},
		#{start_date,jdbcType=VARCHAR},
		#{end_date,jdbcType=VARCHAR},
		#{phone_number,jdbcType=VARCHAR},
		#{uniqueid,jdbcType=VARCHAR},
		#{status,jdbcType=VARCHAR},
		#{achieve_date,jdbcType=VARCHAR},
		#{call_time,jdbcType=VARCHAR},
		#{ring_time,jdbcType=VARCHAR},
		#{talk_time,jdbcType=VARCHAR},
		#{customer_type,jdbcType=VARCHAR},
		#{download,jdbcType=VARCHAR},
		#{callTimes,jdbcType=VARCHAR}

		)
	</insert>

 <select id="findforDetail" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
        from ai_report_detail
		where 
		<if test="list_id != null">
		list_id = #{list_id,jdbcType=VARCHAR}
		</if>
		<if test="phone_number != null">
		and phone_number = #{phone_number,jdbcType=VARCHAR}
		</if>
	</select>


	<select id="findBySQLforDetail" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from
		ai_report_detail
		${findBySQLforDetail}

	</select>
	<select id="findBySQLforCallTimes" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from
		ai_report_detail
		${"findBySQLforCallTimes"}

	</select>


	<select id="findBySQLforDetailByCLSP" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from ai_report_detail
		<if test="campaign_id!=null">
			WHERE campaign_id = #{campaign_id,jdbcType=VARCHAR}
		</if>
		<if test="list_id!=null">
			and list_id = #{list_id,jdbcType=VARCHAR}
		</if>
		<if test="phone_number!=null">
			and phone_number = #{phone_number,jdbcType=VARCHAR}
		</if>
		<if test="start_date!=null">
			and start_date = #{start_date,jdbcType=VARCHAR}
		</if>


	</select>


	<select id="findBySQLforDetailByULS" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from ai_report_detail
		<if test="lead_id!=null">
			WHERE lead_id = #{lead_id,jdbcType=VARCHAR}
		</if>
		<if test="status!=null">
			and status = #{status,jdbcType=VARCHAR}
		</if>
		<if test="uniqueid!=null">
			and uniqueid = #{uniqueid,jdbcType=VARCHAR}
		</if>



	</select>




</mapper>